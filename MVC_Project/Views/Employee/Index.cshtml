@model IEnumerable<MVC_Project.Models.Employee>
@{
}
<a asp-action="Create" asp-controller="Employee" class="btn btn-outline-danger">Create</a>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>Mobile</th>
    </tr>
    @foreach(var emp in Model)
            {
<tr>
    <td>@emp.Id</td>
    <td>@emp.Name</td>
    <td>@emp.Email</td>
    <td>@emp.Gender</td>
    <td>@emp.Mobile</td>
</tr>
            }
</table>